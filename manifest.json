{
  "manifest_version": 3,
  "name": "Free Blocks",
  "permissions" : [
    "contextMenus",
    "clipboardWrite",
    "activeTab",
    "tabs"
  ],

  "version": "1.0",
  "description": "Harness the power of sharing your availability.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/run.js"],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}